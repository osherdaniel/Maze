<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="/View/MainStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.WinningController">
    <children>
        <ImageView fx:id="winningImage" fitHeight="500.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="newMazeButton" layoutX="90.0" layoutY="457.0" mnemonicParsing="false" onAction="#newMazeSelected" text="New Maze" />
      <Button fx:id="exitButton" layoutX="361.0" layoutY="457.0" mnemonicParsing="false" onAction="#exitButtonSelected" text="Exit" />
    </children>
</AnchorPane>
